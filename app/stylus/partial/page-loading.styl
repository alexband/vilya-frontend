@import './spinner.styl'

#page-loading
  background: #eee
  z-index: 99999
  position: fixed
  top: 0
  bottom: 0
  left: 0
  right: 0

  .container
    width: 360px

  .center
    text-align: center

  h1
    text-align: center
    font-weight: 100
    color: fore-color

  small
    color: light-font-color

